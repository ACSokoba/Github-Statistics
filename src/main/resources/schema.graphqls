type Link {
  url: String!
  description: String!
}
type User {
  login: String!
  name: String
}

type Company{
  companyName: String!
  numberOfMembers: Int
  numberOfRepositories: Int
}

type Query {
  allLinks: [Link]
  allUsers: [User]
  getCompanyData: [Company]
}

type Mutation {
  createLink(url: String!, description: String!): Link
}

schema {
  query: Query
  mutation : Mutation
}
